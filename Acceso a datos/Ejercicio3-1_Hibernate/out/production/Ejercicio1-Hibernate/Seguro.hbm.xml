<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Seguro" table="Seguro" >
        <id name="idSeguro" column="idSeguro" type="integer">
            <generator class="identity"></generator>
        </id>
        <property name="nif" />
        <property name="nombre" />
        <property name="apellido1" />
        <property name="apellido2" />
        <property name="edad" />
        <property name="numHijos" />
        <property name="fechaCreacion" />
        <set name="asistenciasMedicas"  cascade="all" inverse="true"  >
            <key>
                <column name="idSeguro"  />
            </key>
            <one-to-many class="AsistenciaMedica" />
        </set>
    </class>
</hibernate-mapping>